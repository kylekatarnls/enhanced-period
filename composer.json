{
  "name": "cmixin/enhanced-period",
  "description": "Carbon mixin to convert `Carbon\\CarbonPeriod` to `Spatie\\Period\\Period` and vice versa",
  "type": "library",
  "require": {
    "php": "^7.2 || ^8.0",
    "nesbot/carbon": "^2.23.0-beta.1",
    "spatie/period": "^1.4.3 || ^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.14 || ^9.3.8"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "KyleKatarn",
      "email": "carbon@selfbuild.fr"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "EnhancedPeriod\\Laravel\\ServiceProvider"
      ]
    }
  }
}
